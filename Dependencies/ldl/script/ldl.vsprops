	ProjectType="Visual C++"
	Version="8.00"
	Name="ldl"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="$(ldl)\include"
	/>
	<UserMacro
		Name="ldl"
		Value="$(SolutionDir)\Dependencies\ldl"
	/>
</VisualStudioPropertySheet>
